<?xml version="1.0" encoding="utf-8"?>
<resources>

    <declare-styleable name="RectangleCardView">
        <attr name="heightPerWidth" format="float" />
    </declare-styleable>

    <declare-styleable name="BaseRatingBar">
        <attr name="srb_numStars" format="integer" />
        <attr name="srb_minimumStars" format="float" />
        <attr name="srb_rating" format="float" />
        <attr name="srb_starSpacing" format="dimension" />
        <attr name="srb_drawableEmpty" format="reference" />
        <attr name="srb_drawableFilled" format="reference" />
        <attr name="srb_isIndicator" format="boolean" />
        <attr name="srb_scrollable" format="boolean" />
        <attr name="srb_clickable" format="boolean" />
        <attr name="srb_clearRatingEnabled" format="boolean" />
        <attr name="srb_starWidth" format="dimension" />
        <attr name="srb_starHeight" format="dimension" />
        <attr name="srb_stepSize" format="float" />
    </declare-styleable>

    <declare-styleable name="RippleImageView">
        <attr name="riv_padding_ripple" format="dimension"/>
        <attr name="riv_icon_ripple" format="reference"/>
        <attr name="riv_ripple_color" format="reference|color"/>
        <attr name="riv_circle_ripple" format="reference|boolean"/>
    </declare-styleable>

    <declare-styleable name="CoreToolbarView">
        <attr name="ctv_ic_back_show" format="boolean"/>
        <attr name="ctv_ic_back_icon" format="reference"/>
        <attr name="ctv_ic_back_enable" format="boolean"/>

        <attr name="ctv_tv_title" format="reference|string"/>
        <attr name="ctv_tv_title_show" format="reference|boolean"/>
        <attr name="ctv_tv_title_text_color" format="reference|color"/>

        <attr name="ctv_ic_up_down_up_show" format="boolean"/>
        <attr name="ctv_ic_up_down_up_icon" format="reference"/>
        <attr name="ctv_ic_up_down_down_icon" format="reference"/>

        <attr name="ctv_tv_action_show" format="boolean"/>
        <attr name="ctv_tv_action_enable" format="boolean"/>
        <attr name="ctv_tv_action_text" format="reference"/>
        <attr name="ctv_tv_action_background" format="reference"/>
        <attr name="ctv_tv_action_background_show" format="boolean"/>
        <attr name="ctv_tv_action_text_color" format="reference|color"/>
        <attr name="ctv_tv_action_text_size" format="dimension"/>

        <attr name="ctv_ic_action_show" format="boolean"/>
        <attr name="ctv_ic_action_icon" format="reference"/>
        <attr name="ctv_ic_action_enable" format="boolean"/>

        <attr name="ctv_ic_action_extra_show" format="boolean"/>
        <attr name="ctv_ic_action_extra_icon" format="reference"/>
        <attr name="ctv_ic_action_extra_enable" format="boolean"/>
    </declare-styleable>
</resources>